{
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/no-extraneous-dependencies": [
            "error",
            { "devDependencies": true }
        ]
    }
}
